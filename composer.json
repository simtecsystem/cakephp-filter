{
    "name": "simtecsystem/cakephp-filter",
    "description": "Filter plugin for CakePHP >3.6",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.6.0 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "BRFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BRFilter\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
